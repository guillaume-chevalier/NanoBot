<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="TP2_Prog.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <system.diagnostics>
        <sources>
            <!-- Cette section définit la configuration de l'enregistrement dans le fichier journal de My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Supprimez les marques de commentaire dans la section suivante pour écrire dans le journal des événements de l'application -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Supprimez les marques de commentaire dans la section suivante et remplacez APPLICATION_NAME par le nom de votre application à écrire dans le journal des événements de l'application -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <userSettings>
        <TP2_Prog.My.MySettings>
            <setting name="screenWidth" serializeAs="String">
                <value>900</value>
            </setting>
            <setting name="screenHeight" serializeAs="String">
                <value>600</value>
            </setting>
            <setting name="fullscreen" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="graphicSettingsChanged" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="graphicQuality" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="debugOn" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="nomJoueur" serializeAs="String">
                <value />
            </setting>
            <setting name="lvlVitesseVoiture" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="selectedLevel" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="calledFrom" serializeAs="String">
                <value />
            </setting>
            <setting name="score" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="UnlockedLevels" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="lvlAccelerationVoiture" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="lvlForceRotation" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="lvlBatteriePower" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="totalScore" serializeAs="String">
                <value>100</value>
            </setting>
            <setting name="oldLvlVitesse" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="oldLvlAcceleration" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="oldLvlForceRotation" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="oldLvlBatteriePower" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="totalTempsJeu" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="showCollisions" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="nbRoues" serializeAs="String">
                <value>1</value>
            </setting>
        </TP2_Prog.My.MySettings>
    </userSettings>
</configuration>
